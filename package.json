{
  "name": "z",
  "version": "1.0.0",
  "description": "基于 EggJS + TypeScript 的应用",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-z --sticky",
    "stop": "egg-scripts stop --title=egg-server-z",
    "dev": "egg-bin dev -r egg-ts-helper/register --sticky",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint .",
    "clean": "ets clean",
    "migrate:new": "egg-sequelize migration:create --name",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "egg": "^2.6.0",
    "egg-graphql": "^2.1.0",
    "egg-jwt": "^3.0.0",
    "egg-redis": "^2.0.0",
    "egg-router-plus": "^1.2.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^3.1.2",
    "egg-session-redis": "^1.0.0",
    "egg-socket.io": "^4.0.7",
    "egg-validate": "^1.0.0",
    "egg-view-ejs": "^2.0.0",
    "ip2region": "^1.1.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.4",
    "raw-body": "^2.3.2",
    "rd": "^1.1.2",
    "uuid": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "@types/supertest": "^2.0.4",
    "@types/bcryptjs": "^2.4.1",
    "@types/ioredis": "^3.2.7",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/lodash": "^4.14.106",
    "@types/nodemailer": "^4.6.0",
    "@types/sequelize": "^4.27.14",
    "@types/socket.io": "^1.4.33",
    "@types/xml2js": "^0.4.2",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^1.8.0",
    "egg-bin": "^4.6.3",
    "egg-mock": "^3.17.0",
    "egg-ts-helper": "^1.5.2",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
